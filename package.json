{
  "name": "@folder/picomatch",
  "description": "Tiny, minimalist glob matching library written in JavaScript.",
  "version": "1.0.0",
  "homepage": "https://github.com/folder/picomatch",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "folder/folder",
  "bugs": {
    "url": "https://github.com/folder/picomatch/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "mocha",
    "cover": "nyc --reporter=text --reporter=html mocha"
  },
  "devDependencies": {
    "ansi-colors": "^2.0.5",
    "bash-match": "^1.0.2",
    "fill-range": "^6.0.0",
