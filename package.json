{
  "name": "picomatch",
  "description": "Tiny, minimalist glob matching library written in JavaScript.",
  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/picomatch",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/picomatch",
  "bugs": {
    "url": "https://github.com/jonschlinkert/picomatch/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "devDependencies": {
    "multimatch": "^2.1.0",
    "micromatch": "^3.1.9",
    "minimatch": "^3.0.4",
    "nanomatch": "^1.2.9",
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.5.3"
  },
  "keywords": [
    "glob",
    "match",
    "picomatch"
  ],
  "lintDeps": {
    "devDependencies": {
      "files": {
        "options": {
          "ignore": [
            "benchmark/**"
          ]
        }
      }
    }
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
