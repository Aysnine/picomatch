    "micromatch": "^3.1.10",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "multimatch": "^2.1.0",
    "nyc": "^12.0.2",
    "time-require": "github:jonschlinkert/time-require"
  },
  "keywords": [
    "glob",
    "match",
    "picomatch"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
